            <div class="View__main row">
              <div class="View__video col-sm-8">
                <video id="video" class="View__videoContent" src="movies/movie1.mov"></video>
                <div class="View__buttons">
                  <input class="btn btn-default" type="button" value="Play" onClick="playVideo()">
                  <input class="btn btn-default" type="button" value="Stop" onClick="pauseVideo()">
                  <input class="btn btn-default" type="button" value="Volume Up" onClick="upVolume()">
                  <input class="btn btn-default" type="button" value="Volume Down" onClick="downVolume()">
                </div>
              </div>

              <div class="View__listener col-sm-4">
                <div class="View__comment">
                  <!-- コメント入力欄 -->
                  <form class="form-inline" action="">
                    <div class="form-group">
                      <input class="form-control" id="message" type="text" placeholder="コメントを入力">
                    </div>
                    <button class="btn btn-default View__formButton" onclick="SendMsg()">送信</button>
                  </form>
                  <ul class="View__commentLists">
                  </ul>
                </div>

